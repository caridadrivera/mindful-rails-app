
<%= flash[:notice] %>

<div class="container">
    <%= form_for(@user) do |f| %>
      <%= f.label :email, "Email:" %>
      <%= f.email_field :email, require: true %><br>

      <%= f.label :password, "Password:" %>
      <%= f.password_field :password, require: true %><br>

      <%= f.label :name, "Name:" %>
      <%= f.text_field :name,require: true %><br>

      <%= f.label :age, "Age:" %>
      <%= f.number_field :age, require: true %><br>

      <%= f.label :about, "About:" %><br>
      <%= f.text_area :about, require: true%>
      <br /><br />
      <%= f.submit "Sign Up" %>

    <% end %>
</div>
