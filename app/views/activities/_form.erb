<%= form_for @activity do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %><br>
  <%= f.label :description %>
  <%= f.text_field :description %><br>
  <%= f.label :length, "Estimated Time: " %>
  <%= f.number_field :length %> mins<br>
  <%= f.label :img_url, "Picture URL: "%>
  <%= f.text_field :img_url %>
  <%= f.submit %>
<% end %>
